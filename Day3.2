
dosya = open("input.txt","r")
oxygen = []
co2 = []
for satir in dosya:
  oxygen.append(satir)
  co2.append(satir)

onesum=0
twosum=0

newlistoxygen=[]
newlistco2=[]


for s in range(0,len(oxygen[0])-1):
  if len(oxygen)==1:
    break
  for i in oxygen:
   if i[s]=='1':
     onesum+=1
   else:
     twosum+=1  
  
  if onesum>=twosum:
    for u in oxygen:
      if u[s]=="1":
        newlistoxygen.append(u)
    oxygen = newlistoxygen.copy()
    newlistoxygen.clear()
    onesum=0
    twosum=0
  else:
    for u in oxygen:
      if u[s]!="1":
        newlistoxygen.append(u)
    oxygen = newlistoxygen.copy()
    newlistoxygen.clear()
    onesum = 0
    twosum = 0

onesum=0
twosum=0

for s in range(0,len(co2[0])-1):
  if len(co2)==1:
    break
  for i in co2:
   if i[s]=='1':
     onesum+=1
   else:
     twosum+=1
  
  if onesum>=twosum:
    for u in co2:
      if u[s]=="0":
        newlistco2.append(u)
    co2 = newlistco2.copy()
    newlistco2.clear()
    onesum=0
    twosum=0
  else:
    for u in co2:
      if u[s]=="1":
        newlistco2.append(u)
    co2 = newlistco2.copy()
    newlistco2.clear()
    onesum=0
    twosum=0

print(f" oxygen list is ",oxygen)
print(f" co2 list is ",co2)


toplam1=0
for i in range(0,len(oxygen[0])-1):
  if oxygen[0][i]=="1":
     toplam1+=pow(2,((len(oxygen[0])-i)-2))

toplam2=0
for i in range(0,len(co2[0])-1):
  if co2[0][i]=="1":
     toplam2+=pow(2,((len(co2[0])-i)-2))

print(toplam1*toplam2)
